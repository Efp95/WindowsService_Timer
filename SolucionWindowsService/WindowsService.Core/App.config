        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>

    <!-- ServiceSetup -->
    <add key="ServiceInterval" value="30000" />
    
  </appSettings>
  
  <connectionStrings>

    <!-- ConnectionString - Change credentials as you need (Do not use 'Integrated Security') -->
    <add name="DbService" providerName="System.Data.SqlClient"
         connectionString="Server=.; Database=DbService; uid=sa; pwd=sql" />
    
  </connectionStrings>
  
</configuration>